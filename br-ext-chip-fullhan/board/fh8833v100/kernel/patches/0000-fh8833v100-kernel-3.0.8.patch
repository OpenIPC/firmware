+	if (write > read || write < read - 2) {
+		RTC_PRINT_DBG("ERROR: read(%d-%d-%d %d:%d:%d) vs "
+				"write(%d-%d-%d %d:%d:%d)\n",
+			   tm0->tm_year + 1900,
+			   tm0->tm_mon + 1,
+			   tm0->tm_mday,
+			   tm0->tm_hour,
+			   tm0->tm_min,
+			   tm0->tm_sec,
+			   tm1->tm_year + 1900,
+			   tm1->tm_mon + 1,
+			   tm1->tm_mday,
+			   tm1->tm_hour,
+			   tm1->tm_min,
+			   tm1->tm_sec);
+		return -1;
+	}
+	return 0;
