+	struct fh_otg_pcd *pcd;
+
+	/** Pointer to the HCD structure. */
+	struct fh_otg_hcd *hcd;
+
+	/** Flag to indicate whether the common IRQ handler is installed. */
+	uint8_t common_irq_installed;
+
+} fh_otg_device_t;
