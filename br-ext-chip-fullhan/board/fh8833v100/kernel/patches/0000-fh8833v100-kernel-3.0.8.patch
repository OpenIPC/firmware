+	if (!ep->desc || ep->fh_ep.num == 0) {
+		FH_WARN("bad ep\n");
+		return -FH_E_INVALID;
+	}
+	fh_ep = &ep->fh_ep;
+
+	return fh_ep->pkt_cnt;
+}
