+#ifdef FH_CLOCK_DEBUG
+static void __clk_sel_ddr_clk(int source)
+{
+	unsigned int clk_sel;
+	int shift = 24;
+	clk_sel = clk_get_clk_sel();
+	clk_sel &= ~(0x1 << shift);
+	clk_sel |= (source & 0x1) << shift;
+	clk_set_clk_sel(clk_sel);
+}
