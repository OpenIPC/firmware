+struct my_data{
+	struct work_struct my_work;
+	int value;
+	u8 buff;
+	u8 len;
+	void __iomem *base;
+	};
+struct my_data *rx_md,*tx_md;
+struct work_struct rx_work_queue,tx_work_queue;
