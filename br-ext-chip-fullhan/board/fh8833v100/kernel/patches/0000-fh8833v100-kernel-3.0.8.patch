+static int sd_init(unsigned int slot_id, void *data, void *v)
+{
+	u32 reg;
+
+	reg = slot_id ? 0xfffffffd : 0xfffffffb;
+	fh_pmu_set_reg(REG_PMU_SWRST_AHB_CTRL, reg);
+	while (fh_pmu_get_reg(REG_PMU_SWRST_AHB_CTRL) != 0xffffffff)
+		;
+	return 0;
+}
