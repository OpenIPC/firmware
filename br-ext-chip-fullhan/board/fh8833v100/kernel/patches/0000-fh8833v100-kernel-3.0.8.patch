+    if (waitqueue_active(&audio_config->writequeue))
+    {
+        int avail = avail_data_len(playback,audio_config);
+        if (avail > audio_config->playback.cfg.period_bytes)
+        {
+            wake_up_interruptible(&audio_config->writequeue);
+        }
+    }
+}
