+//            writel(tx_status, fh_audio_module.regs + ACW_TXFIFO_CTRL);/*tx fifo disable*/
+//#endif
+            break;
+        case AC_AO_RESUME:
+			if (infor_record.play_pid != pid) {
+				return -EBUSY;
+			}
+            printk(KERN_INFO "playback resume\n");
+            audio_config->playback.state = STATE_RUN;
+//#ifndef CONFIG_MACH_FH8830_FPGA
