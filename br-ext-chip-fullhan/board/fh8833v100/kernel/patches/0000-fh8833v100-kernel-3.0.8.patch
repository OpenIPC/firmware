+	} while ((status & BOTH_EMPTY) != BOTH_EMPTY);
+
+	Uart_Init(&myown_port->fh_info);
+	if ((ret = request_irq(myown_port->fh_info.irq_num, fh_uart_isr, 0, FH_DEV_NAME, (void*)myown_port)))
