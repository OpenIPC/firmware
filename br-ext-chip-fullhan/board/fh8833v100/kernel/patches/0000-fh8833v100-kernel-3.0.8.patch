+/**
+ * Function is called to write ADP registers
+ */
+void fh_otg_adp_modify_reg(fh_otg_core_if_t * core_if, uint32_t clr,
+			    uint32_t set)
+{
+	fh_otg_adp_write_reg(core_if,
+			      (fh_otg_adp_read_reg(core_if) & (~clr)) | set);
