+static int get_frame_number(struct usb_hcd *hcd)
+{
+	fh_otg_hcd_t *fh_otg_hcd = hcd_to_fh_otg_hcd(hcd);
+
+	return fh_otg_hcd_get_frame_number(fh_otg_hcd);
+}
