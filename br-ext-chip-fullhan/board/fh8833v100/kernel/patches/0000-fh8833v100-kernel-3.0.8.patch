+	for (i = 0; i < MAX_FRLIST_EN_NUM; i++) {
+		FH_PRINTF("%x\t", hcd->frame_list[i]);
+		if (!(i % 8) && i)
+			FH_PRINTF("\n");
+	}
+	FH_PRINTF("\n----\n");
+
+}
