+//init test data
+struct my_data *init_data(struct my_data *md,struct work_struct  work_queue)
+{
+md = (struct my_data *)kmalloc(sizeof(struct my_data),GFP_KERNEL);
+md->my_work=work_queue;
+return md;
+}
