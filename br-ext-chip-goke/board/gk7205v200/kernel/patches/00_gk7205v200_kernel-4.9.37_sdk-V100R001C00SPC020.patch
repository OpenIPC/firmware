+static int femac_net_ioctl(struct net_device *dev,
+		  struct ifreq *ifreq, int cmd)
+{
+	if (!netif_running(dev)) {
+		return -EINVAL;
+	}
+
+	if (!dev->phydev) {
+		return -EINVAL;
+	}
+
+	return phy_mii_ioctl(dev->phydev, ifreq, cmd);
