+static unsigned int io_sdio1_data_reg[IO_CFG_SDIO1_DATA_LINE_COUNT] = {
+	REG_CTRL_SDIO1_DATA0, REG_CTRL_SDIO1_DATA1,
+	REG_CTRL_SDIO1_DATA2, REG_CTRL_SDIO1_DATA3
+};
