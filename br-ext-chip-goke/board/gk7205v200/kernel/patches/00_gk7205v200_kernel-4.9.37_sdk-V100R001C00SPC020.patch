+static void bsp_set_emmc_ctrl(struct sdhci_host *host)
+{
+	unsigned int reg;
+
+	reg = sdhci_readl(host, SDHCI_EMMC_CTRL);
+	reg |= SDHCI_CARD_IS_EMMC;
+	sdhci_writel(host, reg, SDHCI_EMMC_CTRL);
+}
