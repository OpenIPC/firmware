+# 支持/proc/config.gz
+#
+CONFIG_IKCONFIG=y
+CONFIG_IKCONFIG_PROC=y
+#
+# Performance monitor support(perf)
+#
+CONFIG_HW_PERF_EVENTS=y
+CONFIG_PERF_EVENTS=y
+CONFIG_HAVE_HW_BREAKPOINT=y
+CONFIG_ARM_PMU=y
+#
+# misc debug
+#
+CONFIG_ARM_UNWIND=y
+CONFIG_DEBUG_LL=y
+CONFIG_DEBUG_LL_INCLUDE="debug/8250.S"
